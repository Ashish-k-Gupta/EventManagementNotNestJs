{
  "dependencies": {
    "bcrypt": "^6.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "cls-hooked": "^4.2.2",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "fast-glob": "^3.3.3",
    "http-status-codes": "^2.3.0",
    "jsonwebtoken": "^9.0.2",
    "jwt-token": "^1.0.9",
    "nodemailer": "^7.0.5",
    "pg": "^8.16.3",
    "qrcode": "^1.5.4",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.3.25",
    "typeorm-extension": "^3.7.1",
    "uuid": "^11.1.0",
    "zod": "^3.25.67"
  },
  "scripts": {
    "drop:dev": "npx typeorm-ts-node-commonjs schema:drop -d src/AppDataSource.ts",
    "start": "node ./dist/app.js",
    "build": "tsc",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:generate": "typeorm-ts-node-commonjs -d src/AppDataSource.ts migration:generate",
    "migrate:dev": "npx typeorm-ts-node-commonjs migration:run -d src/AppDataSource.ts",
    "migrate:prod": "npx typeorm migration:run -d dist/AppDataSource.js",
    "start:dev": "nodemon ./src/app.ts",
    "seed:dev": "ts-node src/seed.ts",
    "seed:prod": "node dist/seed.js",
    "build:old": "tsc -p .",
    "start:prod": "npm run migrate:prod && node ./dist/app.js"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cls-hooked": "^4.3.9",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.0.13",
    "@types/nodemailer": "^6.4.17",
    "@types/qrcode": "^1.5.5",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.2"
  }
}